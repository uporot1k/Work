header
    position relative

.nav-container
    display flex
    justify-content center

.nav-box
    width 100%
    background-color $color-1
    opacity 0.77
    z-index 100

.nav-bar
    display flex
    width 100%
    padding-top 20px

nav
    width 100%
    display flex
    align-items center
    padding-left 45px
    padding-top 30px

    li
        &:hover
            >.submenu
                transform rotateX(0deg)
                visibility visible
                opacity 1
                background-color $color-1

.header-nav
    width 100%
    display flex
    justify-content space-between
    font-family "HelveticaNeueLTW1G-Lt"
    font-weight 400
    font-size 18px

    li
        position relative
        padding-bottom 18px

        >a
            color $color-2
            white-space nowrap

.submenu
    position fixed
    width 100%
    top 86px
    left 0
    visibility hidden
    overflow hidden
    opacity 0

    li
        padding 5px 5px
        padding 5px 15px

.submenu-container
    display flex

.subcity
    padding-left 185px
    font-size 14px

    ul
        li
            &:first-child
                color $color-2
                font-family "HelveticaNeueLTW1G-Bd"
                font-size 18px
                font-weight 400
                line-height 30px

.subdirection
    font-size 14px
    padding-left 185px

.subcountry
    font-size 14px
    padding-left 185px

.header-slaider-box
    width 100%
    box-shadow: 0 0 16px rgba(0, 0, 0, 0.83);
    .viewport
        display flex
        justify-content center
        position relative
        overflow hidden
        height 541px
        .main-detail-img
            width 100%
            img
                width auto 
                height auto
                max-width inherit
        .detail-img-caption
            z-index 3
            max-width 815px
            position: absolute
            top 14.5%
            .img-caption-word
                font-size: 60px
            .img-caption-name
                margin-top: 31px
                line-height: 34px
                font-size: 30px
            .img-caption-date
                line-height: 34px
                font-size: 30px
            .img-caption-body
                
                line-height: 21px
                margin-top: 18px
                font-size: 18px
                font-family: "HelveticaNeueLTW1G-Lt"
                text-align: justify
                display flex
                flex-direction: column

            div
                display flex
                justify-content center
                font-family: "HelveticaNeueLTW1G-Th"
                color $color-1
        .header-substrate
            position: absolute
            top 0
            z-index 2
            background black
            width 100%
            height 100%
            opacity 0.4





.sub
    input
        background-color $color-4
        color $color-1
        border 1px solid $color-1

    input::-webkit-input-placeholder
        color $color-1

    input::-moz-placeholder
        color $color-1

.first
    padding-left 5px

    &:before
        content ''
        top 20px
        right 0
        height 20px
        width 15px
        color black
        background-color $color-1
        z-index 100

#subForm
    display none

.sub-form
    display flex
    flex-direction column
    position relative

    *:not(div)
        margin-bottom 10px

.icon-right-open
    position absolute
    right -30px
    bottom -5px
    font-size 30px
    cursor pointer
    color $color-1

.slick-arrow
    display none !important

.slaider-arrow-left
.slaider-arrow-right
    position absolute
    cursor pointer

.header-slaider-box
    .viewport

        .slaider-arrow-left
                top: calc(51%);
                left: 11%;
.header-slaider-box
    .viewport
        .slaider-arrow-right
                top: calc(51%);
                right: 11%;

.icon-left-open-big
.icon-right-open-big
    color $color-1
    font-size 70px
//Конец HEADER!!!!!!!!
.trip-album
    padding: 112px 0 61px 0
    box-shadow: inset 0px -18px 19px -18px rgba(0,0,0,0.38);
    .trip-album-container
        display flex
        justify-content center
        .trip-album-box
            display flex
            justify-content center
            flex-direction: column
            .trip-album-word
                font-family: "HelveticaNeueLTW1G-Th";
                font-size: 60px;
                line-height: 27px;
                text-transform: uppercase
                color $color-2
                display flex
                justify-content center
            .album-slider-box
                margin-top: 67px
                width 100%
                position relative
                .album-slider
                    width: 1147px
                    height 100%
                    .album-slider-item
                        box-shadow: 7px 15px 13px rgba(0, 0, 0, 0.28);
                        width 169px
                        height 167px
                        margin-bottom: 19px
                .slaider-arrow-left
                ,.slaider-arrow-right
                    position absolute
                    top calc(50% - 72px)
                .slaider-arrow-left
                    left -113px
                .slaider-arrow-right
                    right -96px
                .my-dots 
                    position absolute
                    bottom -48px
                    display block
                    width 100%
                    padding 0
                    list-style none
                    text-align center
                    position absolute
                    display block
                    width 100%
                    padding 0
                    list-style none
                    text-align center
                    margin-left: -10px;
                    li 
                        position relative
                        display inline-block

                        margin 0 10px
                        padding 0
                        cursor pointer
                        position relative
                        display inline-block
                        min-width 16px
                        min-height 16px

                        padding 0
                        cursor pointer
                        button 
                            font-size 0
                            line-height 0
                            display block
                            width 16px
                            height 16px
                            padding 5px
                            cursor pointer
                            color #0054b9
                            background-image url('../img/dot.png')
                            
                            border 0px solid #0054b9
                            border-radius 50%
                            outline none
                            border-style none !important
                            background-position 0px 0px
                            &:before 
                                font-family 'slick'
                                font-size 6px
                                line-height 20px
                                position absolute
                                top 2px
                                left 2px
                                width 12px
                                height 12px
                                content ''
                                border-radius 50% 50%
                                text-align center
                                background-color #fff
                                -webkit-font-smoothing antialiased
                                -moz-osx-font-smoothing grayscale
                            
                        &.slick-active 
                            button 
                                &:before 
                                    opacity 0
                                    background-color rgb(255, 255, 255)
                            
                .my-dots li button:hover,
                .my-dots li button:focus 
                    outline none
                .my-dots li button:hover:before,
                .my-dots li button:focus:before 
                    opacity 0
