                height 26px
                width 14px
            
    .accordeon-item-box
        height 100%      
        
        .accordeon-item-body
            height: 792px
            box-shadow: 3px 3px 24px 6px rgba(0, 0, 0, 0.1);
            background-color #fff  
            z-index: 1
            .item-body-img
                box-shadow: 3px 9px 23px rgba(0,0,0,0.1)
            .item-body-caption
                padding: 30px 25px 0 25px
                z-index: 2
                position relative
                max-height 600px;
                color #000000
                font-family "HelveticaNeueLTW1G-Roman"
                font-size 18px
                line-height: 21px

            
            .viewport

                max-height: 100%
                
                position relative
                &::-webkit-scrollbar 
                    height 380px
                    width: 5px;
                &::-webkit-scrollbar-track 
                    background-color #edeadf
                    


                &::-webkit-scrollbar-thumb 
                    
                    background-color #008656

            .item-scroll
                width 5px
                height 380px
                position: absolute
                right -13px
                top 0
                background-color #e1e1e1
                .item-progress
                    position: absolute
                    top 0
                    width: 5px;
                    height: 77px;
                    background-color: #008656;
            p
                line-height: 21px
                padding-right: 13px
                outline  none 
                border  none 
                resize  none 
