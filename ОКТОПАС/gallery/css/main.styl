.fancybox-toolbar
    right: 331px !important
.fancybox-button--zoom
    display none
.fancybox-button
    background: rgba(30,30,30,0)
.fancybox-navigation
    .fancybox-button
        div 
            background: rgba(30,30,30,0)
            height: 98px
            width 50px
            padding: 7px

.fancybox-button
    transition: none
    div
        background: rgba(30,30,30,0);
        svg 
            display none
.fancybox-navigation 
    .fancybox-button--arrow_left
        left 331px
        div
            
            background: transparent
            background-image: url('../img/bg_left.png') !important;
            width 50px
            height 98px
.fancybox-navigation             
    .fancybox-button--arrow_right
        right 331px
        div
            
            background: transparent
            background-image: url('../img/bg_right.png') !important;
            width 50px
            height 98px




@media screen and (min-width : 1980px) 
    .header-slaider-box
        .viewport
            .main-detail-img
                img
                    width: 100%

@media screen and (max-width 1500px)
   .trip-album 
        .trip-album-container
            .trip-album-box
                .album-slider-box
                    .album-slider
                        width 768px
    .tourist-foto
        .tourist-container
            .tourist-box
                .tourist-slider-box
                    .tourist-slider
                         width 768px
@media screen and (max-width: 1280px)
    .map-container
        min-width 550px
    .footer-navbar ul 
        margin-right 75px
	
	
	

