                width 100%
                min-height 445px
                font-family "HelveticaNeueLTW1G-Roman"
                font-size 18px
                font-weight 400
                white-space: pre-wrap
                span
                    color: $color-4
                
            .item-body-offer
                z-index 3
                position relative
                display flex
                height 44px
                width 100%
                justify-content center
                align-items center
                .item-offer-body
                    padding: 3px 6px 6px 6px;
                    width 95px
                    height 26px
                    background-image url('../img/buy.png')
                    background-repeat no-repeat
                    background-position -8px -8px
                    a
                        cursor pointer
                        color $color-1
                        font-family "HelveticaNeueLTW1G-Th"
                        font-size 18px
                &:before
                    position absolute
                    top: 0
                    content: ''
                    width 100%
                    height 2px
                    background-image: linear-gradient(to right, #008656 0%, #ffffff 99%, #ffffff 100%)
        .way

            height 780px
            display flex
            padding: 0 25px
            flex-direction column
            justify-content flex-start
            .item-body-caption
                padding: 49px 0 0 0
                
            .item-body-offer
                margin-top: 60px;
                display block
                &:before
                    position absolute
                    top: 0
                    content: none
                    width 100%
                    height 2px
                    background-image: linear-gradient(to right, #008656 0%, #ffffff 99%, #ffffff 100%)
            :first-child
                font-family "HelveticaNeueLTW1G-Md_0"
                color $color-4
            span
                font-family: "HelveticaNeueLTW1G-Lt";
                font-size 18px
                display flex
                width 100%
                line-height: 21px;
                
            .body-offer-download
                display flex
                width 100%
                margin-top 30px
           
            .item-body-download
                display flex     
                margin-top: 20px
                
            .body-download
                
                width 77px
                height 26px
                background-image url('../img/download.png')
                background-repeat no-repeat
                background-position 100% 100%
                display flex
                justify-content center
                align-items: center
                box-shadow: 0 0 7px 2px rgba(0, 0, 0, 0.14)
                a
                    font-family "HelveticaNeueLTW1G-Th"
                    color: #fff
                    font-size 18px
                    font-weight 400    
        .gid
            
            width 100%
            .item-body-caption
                padding: 30px 25px 40px 25px
            .item-body-offer
                box-shadow: 0 0 24px 6px rgba(0, 0, 0, 0.2), 3px 3px 24px 6px rgba(0, 0, 0, 0.1);
                
                height 55px
                .item-offer-body

                    background-image url('../img/buy_bg.png');
                    background-repeat no-repeat
                    background-position 100% 100%
                    width: 295px
                    height 26px
            .item-body-download
                display flex     
                margin-top: 20px
                margin-left: 23px  
                .body-download
                    width 77px
                    height 26px
                    background-image url('../img/download.png');
                    background-repeat no-repeat
                    background-position 100% 100%
                    display flex
                    justify-content center
                    align-items: center
                    box-shadow: 0 0 7px 2px rgba(0, 0, 0, 0.14)
                    a
                        font-family "HelveticaNeueLTW1G-Th"
                        color: #fff
                        font-size 18px
                        font-weight 400


