        z-index 10
        position relative
        padding 12px 0 11px 10px
        color $color-4
        font-family "HelveticaNeueLTW1G-Th"
        font-size 20px
        font-weight 400
        width 100%
        border-bottom 3px solid $color-4
       
        .slide-show
            position absolute
            left 248px
            bottom 7px
            width 14px
