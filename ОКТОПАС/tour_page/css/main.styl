header
    position relative

.nav-container
    display flex
    justify-content center

.nav-box
    width 100%
    background-color $color-1
    opacity 0.77
    z-index 100

.nav-bar
    display flex
    width 100%
    padding-top 20px

nav
    width 100%
    display flex
    align-items center
    padding-left 45px
    padding-top 30px

    li
        &:hover
            >.submenu
                transform rotateX(0deg)
                visibility visible
                opacity 1
                background-color $color-1

.header-nav
    width 100%
    display flex
    justify-content space-between
    font-family "HelveticaNeueLTW1G-Lt"
    font-weight 400
    font-size 18px

    li
        position relative
        padding-bottom 18px

        >a
            color $color-2
            white-space nowrap

.submenu
    position fixed
    width 100%
    top 86px
    left 0
    visibility hidden
    overflow hidden
    opacity 0

    li
        padding 5px 5px
        padding 5px 15px

.submenu-container
    display flex

.subcity
    padding-left 185px
    font-size 14px

    ul
        li
            &:first-child
                color $color-2
                font-family "HelveticaNeueLTW1G-Bd"
                font-size 18px
                font-weight 400
                line-height 30px

.subdirection
    font-size 14px
    padding-left 185px

.subcountry
    font-size 14px
    padding-left 185px

.header-slaider-box
    width 100%
    box-shadow 11px 9px 8px 2px $color-3

.header-slaider
    li
        width 100%
        position relative

        img
            display block
            max-width 100%
            height auto
            width 100%

.header-slaider-caption
    position absolute
    background-image url('../img/подложка_для_текста.png')
    background-repeat repeat-y
    bottom 0
    padding 32px 0 21px 15px
    left 13.5%
    width 500px

.caption-theme
    color $color-1
    font-family "HelveticaNeueLTW1G-Md_0"
    font-size 28px
    font-weight 400
    text-decoration underline

.caption-link
    color $color-1
    margin-top 15px
    font-family "HelveticaNeueLTW1G-Th"

    a
        color $color-1

.sub
    input
        background-color $color-4
        color $color-1
        border 1px solid $color-1

    input::-webkit-input-placeholder
        color $color-1

    input::-moz-placeholder
        color $color-1

.first
    padding-left 5px

    &:before
        content ''
        top 20px
        right 0
        height 20px
        width 15px
        color black
        background-color $color-1
        z-index 100

#subForm
    display none

.sub-form
    display flex
    flex-direction column
    position relative

    *:not(div)
        margin-bottom 10px

.icon-right-open
    position absolute
    right -30px
    bottom -5px
    font-size 30px
    cursor pointer
    color $color-1

.slick-arrow
    display none !important

.slaider-arrow-left
.slaider-arrow-right
    position absolute
    cursor pointer

.header-slaider-box
    .viewport
        .slaider-arrow-left
                top: calc(51%);
                left: 11%;
.header-slaider-box
    .viewport
        .slaider-arrow-right
                top: calc(51%);
                right: 11%;

.icon-left-open-big
.icon-right-open-big
    color $color-1
    font-size 70px
